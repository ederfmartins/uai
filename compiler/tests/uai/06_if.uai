def int if_else_const(int x) {
    if x > 0 return 1
    else return 0
}

def int if_else_1(int x) {
    if x > 0 return x
    else return x + 1
}

def int if_else_2(int x) {
    if x > 0 return x
    else return 1
}

def int if_else_3(int x) {
    if x > 0 return 1
    else return x
}

def int only_if(int x) {
    if x > 0 return 1
    return -1
}

def int only_if1(int x) {
    if x > 0 return 1
    return x
}

def int only_if2(int x) {
    if x > 0 return x
    return 0
}

def int only_if3(int x) {
    if x > 0 return 1
    return x + 2
}

def main()
{
    print if_else_const(1)
    print if_else_const(-1)

    print if_else_1(1)
    print if_else_1(-1)

    print if_else_2(1)
    print if_else_2(-1)

    print if_else_3(1)
    print if_else_3(-1)

    print only_if(1)
    print only_if(-1)

    print only_if1(1)
    print only_if1(-1)

    print only_if2(1)
    print only_if2(-1)

    print only_if3(1)
    print only_if3(-1)
}